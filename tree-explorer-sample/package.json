{
	"name": "dep",
	"displayName": "Dependency Example",
	"description": "Dependency example for VSCode's explorer API",
	"version": "0.0.1",
	"publisher": "octref",
	"engines": {
		"vscode": "^1.12.0"
	},
	"enableProposedApi": true,
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:nodeDependencies",
		"onView:jsonOutline"
	],
	"main": "./out/src/extension",
	"icon": "media/dep.png",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "nodeDependencies",
					"name": "Node Dependencies"
				},
				{
					"id": "jsonOutline",
					"name": "Json Outline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.1.4",
		"vscode": "^1.0.0",
		"@types/node": "*"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2"
	}
}